{
  "manifest_version": 2,
  "name": "Generator Branch",
  "description": "Default branching based on the title of stories in Jira",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"    
  },
  "permissions": [
    "tabs",
    "https://*/jira/*/*"
  ],  
  "browser_action": {
    "default_title": "Work only with jira issue pages",
    "default_icon": "icon32-off.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}